<link rel="stylesheet/less" type="text/css" href="styles.less" />
<script src="https://cdn.jsdelivr.net/npm/less"></script>

@{
    ViewData["Title"] = "PatosPrint";
}
<h1>Print Ticket</h1>
<form asp-controller="PatosPrint" asp-action="Index" method="post">
    <div class="form-group">
        <label for="input1">Input 1:</label>
        <input class="form-control" type="text" id="input1" name="Input1" />
    </div>
    <div class="form-group">
        <label for="input2">Input 2:</label>
        <input class="form-control" type="text" id="input2" name="Input2" />
    </div>
    <div class="form-group">
        <label for="input3">Input 3:</label>
        <input class="form-control" type="text" id="input3" name="Input3" />
    </div>
    <div class="form-group">
        <label for="input4">Input 4:</label>
        <input class="form-control" type="text" id="input4" name="Input4" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
